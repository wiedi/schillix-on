    hotline="Please contact your local service provider" \
    name="I/O Test Header Files" vendor="Sun Microsystems, Inc." \
    version=11.11,REV=2009.11.11
legacy pkg=SUNWiotu arch=$(ARCH) category=system \
    desc="I/O test utilities and drivers" \
    hotline="Please contact your local service provider" \
    name="I/O test utilities and drivers, (Usr)" \
    vendor="Sun Microsystems, Inc." version=11.11,REV=2009.11.11
license cr_Sun license=cr_Sun
license lic_CDDL license=lic_CDDL
