.BR ddi_dma_addr_setup (9F),
.BR ddi_dma_buf_bind_handle (9F),
\fBddi_dma_buf_setup\fR(9F), \fBddi_dma_setup\fR(9F),
.BR ddi_dma_sync (9F),
.BR ddi_dma_cookie (9S)
.sp
.LP
.I Writing Device Drivers
