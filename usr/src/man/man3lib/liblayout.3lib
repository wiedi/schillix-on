.TH liblayout 3LIB "22 May 2003" "SunOS 5.11" "Interface Libraries"
.SH NAME
liblayout \- layout service library
.SH SYNOPSIS
.LP
.nf
cc [ \fIflag\fR.\|.\|. ] \fIfile\fR.\|.\|. \fB-llayout\fR [ \fIlibrary\fR.\|.\|. ]
#include <\fBsys/layout.h\fR>
.fi

.SH DESCRIPTION
.sp
.LP
Functions in this library provide various layout service routines.
.SH INTERFACES
.sp
.LP
The shared object \fBliblayout.so.1\fR provides the public inter faces
defined below. See \fBIntro\fR(3) for additional information on shared
object interfaces.
.sp

.sp
.TS
tab();
lw(2.75i) lw(2.75i)
lw(2.75i) lw(2.75i)
.
\fBm_create_layout\fR\fBm_destroy_layout\fR
\fBm_getvalues_layout\fR\fBm_setvalues_layout\fR
\fBm_transform_layout\fR\fBm_wtransform_layout\fR
.TE

.SH FILES
.sp
.ne 2
.mk
.na
\fB/usr/lib/liblayout.so.1\fR
.ad
.RS 30n
.rt
shared object
.RE

.sp
.ne 2
.mk
.na
\fB/usr/lib/64/liblayout.so.1\fR
.ad
.RS 30n
.rt
64-bit shared object.
.RE

